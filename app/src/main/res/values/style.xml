<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomDatePickerDialog"
        parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowSwipeToDismiss">false</item>
        <item name="android:windowBackground">#000000</item>
        <item name="android:textColorPrimary">#ffff</item> <!--days of the month-->
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">13.5dp</item>
        <item name="android:textColorSecondary">#42BFB5</item>
        <item name="android:textColorPrimaryInverse">#42BFB5</item>
        <item name="android:colorAccent">#42BFB5</item>

        <item name="android:colorControlActivated">#42BFB5</item>
        <item name="android:colorControlNormal">#42BFB5</item>
        <item name="android:colorControlHighlight">#42BFB5</item>
        <item name="colorAccent">#42BFB5</item>
<item name="android:timePickerDialogTheme">@style/MyDatePickerStyle</item>

    </style>
    <style name="CustomDatePickerDialog1"
        parent="android:Widget.Material.TimePicker">
        <item name="android:textColorPrimary">#ffff</item> <!--days of the month-->
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">13.5dp</item>
        <item name="android:windowSwipeToDismiss">false</item>
        <item name="android:textColorSecondary">#ffff</item>
        <item name="android:colorAccent">#ffff</item>
        <item name="android:colorControlNormal">#42BFB5</item>

    </style>
    <style name="CustomDatePickerDialog12"
        parent="android:Widget.Material.DatePicker">
        <item name="android:textColorPrimary">#ffff</item> <!--days of the month-->
        <item name="android:textAlignment">center</item>
        <item name="android:textSize">13.5dp</item>
        <item name="android:windowSwipeToDismiss">false</item>
        <item name="android:textColorSecondary">#ffff</item>
        <item name="android:colorAccent">#ffff</item>
        <item name="android:colorControlNormal">#42BFB5</item>

    </style>
    <style name="Dialog.Theme" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:textColorPrimary">#42BFB5</item>
        <item name="colorPrimaryDark">#42BFB5</item>
        <item name="colorAccent">#42BFB5</item>
        <item name="android:colorControlNormal">#42BFB5</item>
        <item name="android:divider">#42BFB5</item>
    </style>

    <style name="MyDialogTheme"
        parent="ThemeOverlay.AppCompat.Dark">
        <item name="android:timePickerStyle">@style/MyDatePickerStyle</item>
        <item name="android:background">#000000</item>
        <item name="android:numbersTextColor">#ffff</item>
        <item name="android:buttonBarPositiveButtonStyle">@style/AppTheme.Alert.Button.Positive</item>
        <item name="android:buttonBarNegativeButtonStyle">@style/AppTheme.Alert.Button.Negative</item>
        <item name="android:textSize">13.5dp</item>
    </style>
    <style name="MyDatePickerStyle" parent="Widget.MaterialComponents.TimePicker">
        <item name="android:colorControlNormal">#42BFB5</item>
        <item name="android:colorControlHighlight">#42BFB5</item>
        <item name="android:colorPrimary">#42BFB5</item>
        <item name="android:colorAccent">#42BFB5</item>
        <item name="android:timePickerMode">spinner</item>
        <item name="android:colorControlActivated">#42BFB5</item>
        <item name="android:numberPickerStyle">@style/MatchSpinnerStyle</item>
    </style>
    <style name="MatchSpinnerStyle" parent="android:style/Widget.Material.NumberPicker">
        <item name="android:divider">#42BFB5</item>
        <item name="android:dividerHeight">2dp</item>
        <item name="colorControlNormal">#42BFB5</item>
        <item name="android:spinnerStyle">@style/NumberPicker1</item>
    </style>
    <style name="NumberPicker" parent="ThemeOverlay.MaterialComponents.TimePicker">
        <item name="android:colorControlNormal">#42BFB5</item>
        <item name="android:colorControlHighlight">#42BFB5</item>
        <item name="android:colorPrimary">#42BFB5</item>
        <item name="android:colorAccent">#42BFB5</item>
        <item name="android:divider">#42BFB5</item>
        <item name="android:colorControlActivated">#42BFB5</item>

    </style>
    <style name="NumberPicker1" parent="android:Widget.Material.NumberPicker">
        <item name="android:colorControlNormal">#42BFB5</item>
        <item name="android:colorControlHighlight">#42BFB5</item>
        <item name="android:colorPrimary">#42BFB5</item>
        <item name="android:colorAccent">#42BFB5</item>
        <item name="android:divider">#42BFB5</item>
        <item name="android:colorControlActivated">#42BFB5</item>

    </style>
    <style name="AppTheme.Alert.Button.Positive" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:textColor">#42BFB5</item>

        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textAlignment">center</item>

    </style>

    <style name="AppTheme.Alert.Button.Negative" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:textColor">#ffff</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

</resources>